---
import Layout from '../layouts/Layout.astro';

import topscoreLogo from '../assets/topscore-logo.png';

import '../styles/global.css';
---

<Layout>
	<header>
		<nav
			class="bg-white border-gray-200 dark:bg-gray-900"
			aria-label="Main"
		>
			<div
				class="max-w-[1440px] flex flex-wrap items-center justify-between mx-auto p-4"
			>
				<a
					href="https://flowbite.com/"
					class="flex items-center space-x-3 rtl:space-x-reverse"
				>
					<img src={topscoreLogo.src} class="h-8" alt="TopScore" />
				</a>
				<button
					type="button"
					class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600"
					aria-controls="navbar-default"
					aria-expanded="false"
				>
					<span class="sr-only">Open menu</span>
					<svg
						class="w-5 h-5"
						aria-hidden="true"
						xmlns="http://www.w3.org/2000/svg"
						fill="none"
						viewBox="0 0 17 14"
					>
						<path
							stroke="currentColor"
							stroke-linecap="round"
							stroke-linejoin="round"
							stroke-width="2"
							d="M1 1h15M1 7h15M1 13h15"></path>
					</svg>
				</button>
				<div
					class="hidden w-full md:block md:w-auto"
					id="navbar-default"
				>
					<ul
						class="font-medium flex flex-col p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 rtl:space-x-reverse md:mt-0 md:border-0 md:bg-white dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700"
					>
						<li>
							<a
								href="#"
								class="block py-2 px-3 text-white bg-brand-purple rounded-sm md:bg-transparent md:text-brand-purple md:p-0 dark:text-white md:dark:text-blue-500"
								aria-current="page">Dashboard</a
							>
						</li>
						<li>
							<a
								href="#"
								class="block py-2 px-3 text-gray-900 rounded-sm hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-brand-purple md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent"
								>Link 1</a
							>
						</li>
						<li>
							<a
								href="#"
								class="block py-2 px-3 text-gray-900 rounded-sm hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-brand-purple md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent"
								>Link 2</a
							>
						</li>
						<li>
							<a
								href="#"
								class="block py-2 px-3 text-gray-900 rounded-sm hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-brand-purple md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent"
								>Link 3</a
							>
						</li>
						<li>
							<a
								href="#"
								class="block py-2 px-3 text-gray-900 rounded-sm hover:bg-gray-100 md:hover:bg-transparent md:border-0 md:hover:text-brand-purple md:p-0 dark:text-white md:dark:hover:text-blue-500 dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent"
								>Link 4</a
							>
						</li>
					</ul>
				</div>
			</div>
		</nav>
	</header>

	<main class="bg-gray-200 min-h-screen p-4 md:p-8 space-y-8">
		<div class="max-w-7xl mx-auto">
			<h1 class="text-4xl md:text-6xl font-black text-brand-purple mt-1">
				<span
					class="text-xl md:text-3xl text-black font-normal mt-2 block"
					>BP Test Campaign</span
				>
				Campaign dashboard
			</h1>

			<section class="py-6 md:py-12">
				<h2>Filter campaign</h2>

				<form
					class="flex flex-col md:grid md:grid-cols-2 lg:grid-cols-4 gap-4 md:items-end md:gap-10"
				>
					<div>
						<label for="campaign" class="block text-sm mb-1"
							>Campaign name</label
						>
						<select
							id="campaign"
							class="w-full p-2 rounded text-black"
						>
							<option selected>BP Test Campaign</option>
							<option>Test Campaign 1</option>
							<option>Test Campaign 2</option>
							<option>Test Campaign 3</option>
							<option>Test Campaign 4</option>
						</select>
					</div>

					<div>
						<label for="start-date" class="block text-sm mb-1"
							>Start date</label
						>
						<input
							id="start-date"
							type="date"
							class="w-full p-2 rounded text-black"
							value="2025-05-18"
						/>
					</div>

					<div>
						<label for="end-date" class="block text-sm mb-1"
							>End date</label
						>
						<input
							id="end-date"
							type="date"
							class="w-full p-2 rounded text-black"
							value="2025-05-21"
						/>
					</div>

					<div
						class="mt-4 md:mt-auto col-span-2 md:col-span-1 md:align-self-end"
					>
						<button type="submit" class="px-4 py-2 w-full"
							>Apply Filters</button
						>
					</div>
				</form>
			</section>

			<section class="py-8">
				<h2>The campaign at a glance</h2>

				<div class="grid grid-cols-2 lg:grid-cols-4 gap-4 md:gap-10">
					<div class="bg-white p-6 text-center">
						<svg
							xmlns="http://www.w3.org/2000/svg"
							viewBox="0 0 640 512"
							aria-hidden="true"
							width="40"
							height="40"
							class="mx-auto mb-4 fill-brand-purple"
							><path
								d="M144 0a80 80 0 1 1 0 160A80 80 0 1 1 144 0zM512 0a80 80 0 1 1 0 160A80 80 0 1 1 512 0zM0 298.7C0 239.8 47.8 192 106.7 192l42.7 0c15.9 0 31 3.5 44.6 9.7c-1.3 7.2-1.9 14.7-1.9 22.3c0 38.2 16.8 72.5 43.3 96c-.2 0-.4 0-.7 0L21.3 320C9.6 320 0 310.4 0 298.7zM405.3 320c-.2 0-.4 0-.7 0c26.6-23.5 43.3-57.8 43.3-96c0-7.6-.7-15-1.9-22.3c13.6-6.3 28.7-9.7 44.6-9.7l42.7 0C592.2 192 640 239.8 640 298.7c0 11.8-9.6 21.3-21.3 21.3l-213.3 0zM224 224a96 96 0 1 1 192 0 96 96 0 1 1 -192 0zM128 485.3C128 411.7 187.7 352 261.3 352l117.3 0C452.3 352 512 411.7 512 485.3c0 14.7-11.9 26.7-26.7 26.7l-330.7 0c-14.7 0-26.7-11.9-26.7-26.7z"
							></path></svg
